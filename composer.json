{
  "name": "ayoub-amzil/model-activity-log",
  "description": "A simple Laravel package to log Eloquent model create, update, and delete activities with user tracking.",
  "type": "library",
  "license": "MIT",
  "version": "1.1.0",
  "authors": [
    {
      "name": "Ayoub AMZIL",
      "email": "dev.amzil.ayoub@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "AyoubAmzil\\ModelActivityLog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AyoubAmzil\\ModelActivityLog\\ModelActivityLogServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
